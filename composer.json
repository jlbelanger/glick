{
	"name": "jlbelanger/glick-api",
	"description": "Track occurrences of time-based events.",
	"version": "1.0.0",
	"type": "project",
	"license": "MIT",
	"authors": [
		{
			"name": "Jenny Belanger",
			"email": "mail@jennybelanger.com",
			"homepage": "https://www.jennybelanger.com/"
		}
	],
	"require": {
		"php": "^7.3|^8.0",
		"fruitcake/laravel-cors": "^2.0.4",
		"guzzlehttp/guzzle": "^7.4.1",
		"jlbelanger/laravel-json-api": "dev-master#2b7f10634465c0019d6c3dd17ffd5d6231e48892",
		"laravel/framework": "^8.77.1",
		"laravel/sanctum": "^2.13.0"
	},
	"require-dev": {
		"facade/ignition": "^2.17.3",
		"fakerphp/faker": "^1.17.0",
		"mockery/mockery": "^1.4.4",
		"nunomaduro/collision": "^5.10.0",
		"phpunit/phpunit": "^9.5.11",
		"squizlabs/php_codesniffer": "^3.6.2"
	},
	"autoload": {
		"psr-4": {
			"App\\": "app/",
			"Database\\Factories\\": "database/factories/",
			"Database\\Seeders\\": "database/seeders/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"repositories": [
		{
			"type": "vcs",
			"url": "git@github.com:jlbelanger/laravel-json-api.git"
		}
	],
	"config": {
		"optimize-autoloader": true,
		"preferred-install": "dist",
		"sort-packages": true
	},
	"scripts": {
		"post-autoload-dump": [
			"Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
			"@php artisan package:discover --ansi"
		],
		"post-root-package-install": [
			"@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
		],
		"post-create-project-cmd": [
			"@php artisan key:generate --ansi"
		]
	},
	"extra": {
		"laravel": {
			"dont-discover": []
		}
	}
}
